<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8">
    <id>$id$</id>
    <version>$version$</version>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>https://raw.githubusercontent.com/meriworks/PowerShell.ConfigBuilder/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/meriworks/PowerShell.ConfigBuilder</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
        This nuget package adds a convention to Visual Studio projects to allow 
        building .config files depending on environment. 
        For example, if developers has different databases configured in web.config, 
        this package can support each developer with a unique web.config.
    </description>
    <tags>Meriworks PowerShell Visual Studio Project</tags>
    <releaseNotes>
    </releaseNotes>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src="nuspec\build\**\*.*" target="build\"/>
    <file src="nuspec\content\**\*.*" target="content\"/>
    <file src="nuspec\tools\**\*.*" target="tools\"/>
  </files>
</package>